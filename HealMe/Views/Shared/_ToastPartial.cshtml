@if (TempData["ToastType"] != null)
{
    <script>
        toastr['@TempData["ToastType"]'](
            '@Html.Raw(Json.Serialize(TempData["ToastMessage"]))',
            '@Html.Raw(Json.Serialize(TempData["ToastTitle"]))',
            { timeOut: @(TempData["ToastTimeout"] ?? 2000) }
        );
    </script>
}